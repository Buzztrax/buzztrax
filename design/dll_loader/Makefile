### Tools

CC = winegcc
CXX = wineg++
RC = wrc
WINEBUILD = winebuild


### Generic targets

all: $(SUBDIRS) $(DLLS:%=%.so) $(EXES:%=%.so)

### Build rules

.PHONY: all clean dummy

$(SUBDIRS): dummy
	@cd $@ && $(MAKE)

# Implicit rules

.SUFFIXES: .cpp .rc .res
DEFINCL = $(INCLUDE_PATH) $(DEFINES) $(OPTIONS)

.c.o:
	$(CC) -c $(CFLAGS) $(CEXTRA) $(DEFINCL) -o $@ $<

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(CXXEXTRA) $(DEFINCL) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(CXXEXTRA) $(DEFINCL) -o $@ $<

.rc.res:
	$(RC) $(RCFLAGS) $(RCEXTRA) $(DEFINCL) -fo$@ $<

# Rules for cleaning

CLEAN_FILES     = *.dbg.c y.tab.c y.tab.h lex.yy.c *.o *.so \
                  core *.orig *.rej \
                  \\\#*\\\# *~ *% .\\\#*

clean:: $(SUBDIRS:%=%/__clean__) $(EXTRASUBDIRS:%=%/__clean__)
	$(RM) $(CLEAN_FILES) $(RC_SRCS:.rc=.res) $(C_SRCS:.c=.o) $(CXX_SRCS:.cpp=.o)
	$(RM) $(DLLS:%=%.dbg.o) $(DLLS:%=%.so)
	$(RM) $(EXES:%=%.dbg.o) $(EXES:%=%.so) $(EXES:%.exe=%)

$(SUBDIRS:%=%/__clean__): dummy
	cd `dirname $@` && $(MAKE) clean

$(EXTRASUBDIRS:%=%/__clean__): dummy
	-cd `dirname $@` && $(RM) $(CLEAN_FILES)

### Target specific build rules

$(dll_loader_exe_MODULE).dbg.c: $(dll_loader_exe_C_SRCS) $(dll_loader_exe_CXX_SRCS)
	$(WINEBUILD) -o $@ --debug -C$(SRCDIR) $(dll_loader_exe_C_SRCS) $(dll_loader_exe_CXX_SRCS)

$(dll_loader_exe_MODULE).so: $(dll_loader_exe_MODULE).dbg.o $(dll_loader_exe_OBJS)
	$(CXX) $(dll_loader_exe_LDFLAGS) -o $@ $(dll_loader_exe_OBJS) $(dll_loader_exe_MODULE).dbg.o $(dll_loader_exe_LIBRARY_PATH) $(LIBRARY_PATH) $(dll_loader_exe_DLLS:%=-l%) $(dll_loader_exe_LIBRARIES:%=-l%)


