<?xml version="1.0" encoding="UTF-8"?>

<buzztard
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="buzztard.xsd">

  <meta>
    <info>one buzz generator</info>
    <name>buzz 1</name>
    <genre>dark-wave</genre>
    <bpm>120</bpm>
    <tpb>4</tpb>
    <bars>16</bars>
    <create-dts>2005-09-20T22:10:56Z</create-dts>
    <change-dts>2005-09-20T22:10:56Z</change-dts>
  </meta>
  
  <setup zoom="1.0">
    <machines>
      <sink id="audio_sink">
        <properties>
         <property key="xpos" value="10" />
         <property key="ypos" value="40" />
        </properties>
        <globaldata name="volume" value="20"/>
        <globaldata name="bpm" value="145"/>
      </sink>
      
      <source id="gen" plugin-name="bml-DTMF-1" voices="0">
        <properties>
         <property key="xpos" value="10" />
         <property key="ypos" value="20" />
        </properties>
        <globaldata name="volume" value="1.0"/>
      </source>
    </machines>
    
    <wires>
      <wire src="gen" dst="audio_sink" gain="1.0" pan="-50"/>
    </wires>    
  </setup>
  
  <patterns>
    <pattern id="gen_beeps" machine="gen" name="beeps" length="16">
      <tick time="0">
        <voicedata name="Dial-Number" voice="0" value="1"/>
        <voicedata name="Sustain" voice ="0" value="200"/>
        <voicedata name="Volume" voice="0" value="200"/>
      </tick>
      <tick time="4">
        <voicedata name="Dial-Number" voice="0" value="2"/>
      </tick>
      <tick time="8">
        <voicedata name="Dial-Number" voice="0" value="3"/>
      </tick>
    </pattern>
  </patterns>
  
  <sequence>
    <labels>
      <label time="0" name="start"/>
      <label time="63" name="end"/>
    </labels>
    <tracks>
      <track index="0" machine="gen">
        <position time="0" pattern="gen_beeps"/>
        <position time="32" pattern="gen_beeps"/>
      </track>
    </tracks>
  </sequence>
  

</buzztard>
