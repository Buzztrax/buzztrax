<?xml version="1.0" encoding="UTF-8"?>

<buzztard
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="buzztard.xsd">

  <meta>
    <info>one buzz generator</info>
    <name>buzz 1</name>
    <genre>dark-wave</genre>
    <bpm>120</bpm>
    <tpb>4</tpb>
    <bars>16</bars>
    <create-dts>2005-01-01</create-dts>
    <change-dts>20050101</change-dts>
  </meta>
  
  <setup zoom="1.0">
    <machines>
      <sink id="audio_sink">
        <machineui xpos="7" ypos="5" />
        <globaldata name="volume" value="20"/>
        <globaldata name="bpm" value="145"/>
      </sink>
      
      <source id="gen" pluginname="bml-DTMF-1" voices="0">
        <machineui xpos="10" ypos="20"/>
        <globaldata name="volume" value="1.0"/>
      </source>
    </machines>
    
    <wires>
      <wire src="gen" dst="audio_sink" gain="1.0" pan="-50"/>
    </wires>    
  </setup>
  
  <patterns>
    <pattern id="gen_beeps" machine="gen" name="beeps" length="16">
      <tick time="0">
        <tickdata name="Dial-Number" voice="0" value="1"/>
        <tickdata name="Sustain" voice ="0" value="200"/>
        <tickdata name="Volume" voice="0" value="200"/>
      </tick>
      <tick time="4">
        <tickdata name="Dial-Number" voice="0" value="2"/>
      </tick>
      <tick time="8">
        <tickdata name="Dial-Number" voice="0" value="3"/>
      </tick>
    </pattern>
  </patterns>
  
  <sequence>
    <labels>
      <label time="0" name="start"/>
      <label time="63" name="end"/>
    </labels>
    <tracks>
      <track index="0" machine="gen">
        <position time="0" pattern="gen_beeps"/>
        <position time="32" pattern="gen_beeps"/>
      </track>
    </tracks>
  </sequence>
  

</buzztard>
