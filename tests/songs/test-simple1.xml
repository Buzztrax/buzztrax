<?xml version="1.0" encoding="UTF-8"?>

<buzztard
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="buzztard.xsd">

  <meta>
    <info>what a lovely tune</info>
    <name>test simple 1</name>
    <genre>dark-wave</genre>
    <bpm>100</bpm>
    <tpb>8</tpb>
    <bars>16</bars>
    <create-dts>2005-09-20T22:10:56Z</create-dts>
    <change-dts>2005-09-20T22:10:56Z</change-dts>
  </meta>
  
  <setup zoom="1.0">
    <machines>
      <sink id="audio_sink">
        <machineui xpos="20" ypos="70"/>
        <globaldata name="volume" value="20"/>
        <globaldata name="bpm" value="200"/>
      </sink>
      <source id="sine1" pluginname="sinesrc" voices="0">
        <machineui xpos="50" ypos="30"/>
        <globaldata name="volume" value="1.0"/>
      </source>
    </machines>
    
    <wires>
      <wire src="sine1" dst="audio_sink" gain="1.0"/>
    </wires>    
  </setup>
  
  <patterns>
    <pattern id="sine1_beep" machine="sine1" name="beeps" length="3">
      <tick time="0">
        <tickdata name="freq" voice="0" value="440,0"/>
        <tickdata name="volume" voice="0" value="1,0"/>
      </tick>
      <tick time="1">
        <tickdata name="freq" voice="0" value="880,0"/>
        <tickdata name="volume" voice="0" value="1,0"/>
      </tick>
      <tick time="2">
        <tickdata name="freq" voice="0" value="1760,0"/>
        <tickdata name="volume" voice="0" value="1,0"/>
      </tick>
    </pattern>
  </patterns>
  
  <sequence>
    <labels>
      <label time="0" name="start"/>
      <!--label time="1" name="end"/-->
    </labels>
    <tracks>
      <track index="0" machine="sine1">
        <position time="0" pattern="sine1_beep"/>
      </track>
    </tracks>
  </sequence>
  
</buzztard>
