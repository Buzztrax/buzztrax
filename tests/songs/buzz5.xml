<?xml version="1.0" encoding="UTF-8"?>

<buzztard
  xmlns="http://www.buzztard.org/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="buzztard.xsd">

  <meta>
    <info>a sine followed by an amp, both playing patterns</info>
    <name>simple 2</name>
    <genre>dark-wave</genre>
    <create-dts>2005-09-20T22:10:56Z</create-dts>
    <change-dts>2005-09-20T22:10:56Z</change-dts>
    <bpm>120</bpm>
    <tpb>4</tpb>
    <bars>16</bars>
  </meta>
  
  <setup>
    <machines>
      <machine type="sink" id="audio_sink">
        <properties>
          <property key="xpos" value="0.7"/>
          <property key="ypos" value="0.5"/>
        </properties>
      </machine>

      <machine type="source" id="bass2" plugin-name="bml-Jeskola-Bass-2" voices="1">
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="-0.5"/>
        </properties>
        <patterns>
          <pattern id="bass2_beeps" name="beeps" length="16">
            <tick time="0"><voicedata name="Note" voice="0" value="c-2"/></tick>
            <tick time="4"><voicedata name="Note" voice="0" value="e-2"/></tick>
            <tick time="8"><voicedata name="Note" voice="0" value="g-2"/></tick>
          </pattern>
        </patterns>
      </machine>
    </machines>
    
    <wires>
      <wire src="bass2" dst="audio_sink" gain="1.0" panorama="0.0"/>
    </wires>    
  </setup>
  
  <sequence length="64" tracks="1">
    <labels>
      <label time="0" name="start"/>
      <label time="63" name="end"/>
    </labels>
    <tracks>
      <track index="0" machine="bass2">
        <position time="0" pattern="bass2_beeps"/>
        <position time="32" pattern="bass2_beeps"/>
      </track>
    </tracks>
  </sequence>
  
</buzztard>
