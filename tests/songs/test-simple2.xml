<?xml version="1.0" encoding="UTF-8"?>

<buzztard xmlns="http://buzztard.sourceforge.net/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance" xsd:noNamespaceSchemaLocation="buzztard.xsd"
	name="simple test Song">

	<meta>
		<info>what a lovely tune</info>
    <name>test simple 2</name>
		<genre>dark-wave</genre>
    <bpm>100</bpm>
    <tpb>8</tpb>
    <bars>16</bars>
	</meta>
	
  <setup>
    <machines>
      <sink id="audio_sink">
      </sink>
      <source id="sine1" pluginname="sinesrc" voices="0">
      </source>
    </machines>
    
    <wires>
      <wire src="sine1" dst="audio_sink" gain="1.0"/>
    </wires>    
  </setup>
  
  <patterns>
  </patterns>
  
  <sequence>
    <labels>
      <label time="0" name="start"/>
      <!--label time="1" name="end"/-->
    </labels>
    <tracks>
      <track index="0" machine="sine1">
      </track>
    </tracks>
  </sequence>
  
  <wavetable/>

</buzztard>

