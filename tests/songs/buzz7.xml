<?xml version="1.0" encoding="UTF-8"?>

<buzztard
  xmlns="http://www.buzztard.org/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="buzztard.xsd">

  <meta>
    <info>three buzz generators and one buzz effect</info>
      <name>buzz 7</name>
      <genre>dark-wave</genre>
      <bpm>120</bpm>
      <tpb>4</tpb>
      <bars>16</bars>
      <create-dts>2005-09-20T22:10:56Z</create-dts>
      <change-dts>2005-09-20T22:10:56Z</change-dts>
  </meta>
  
  <setup>
    <machines>
      <machine type="sink" id="audio_sink">
        <properties>
          <property key="xpos" value="0.7"/>
          <property key="ypos" value="0.0"/>
        </properties>
        <globaldata name="volume" value="20"/>
        <globaldata name="bpm" value="145"/>
      </machine>

      <machine type="processor" id="chorus" plugin-name="bml-Jeskola-Chorus" voices="0">
        <properties>
          <property key="xpos" value="0.0"/>
          <property key="ypos" value="0.0"/>
        </properties>
      </machine>

      <machine type="source" id="bd" plugin-name="bml-ErsKick" voices="1">
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="-0.5"/>
        </properties>
        <globaldata name="volume" value="1.0"/>
        <patterns>
          <pattern id="bd_beats" machine="bd" name="beats" length="16">
            <tick time= "0"><voicedata name="Trig" voice="0" value="1"/></tick>
            <tick time= "4"><voicedata name="Trig" voice="0" value="1"/></tick>
            <tick time= "8"><voicedata name="Trig" voice="0" value="1"/></tick>
            <tick time="12"><voicedata name="Trig" voice="0" value="1"/></tick>
          </pattern>
        </patterns>
      </machine>

      <machine type="source" id="snr" plugin-name="bml-ErsSnare" voices="1">
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="0.0"/>
        </properties>
        <globaldata name="volume" value="1.0"/>
        <patterns>
          <pattern id="snr_beats" machine="snr" name="beats" length="16">
            <tick time= "4"><voicedata name="Trig" voice="0" value="1"/></tick>
            <tick time="12"><voicedata name="Trig" voice="0" value="1"/></tick>
          </pattern>
        </patterns>
      </machine>

      <machine type="source" id="hh" plugin-name="bml-ErsHihat" voices="1">
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="0.5"/>
        </properties>
        <globaldata name="volume" value="1.0"/>
      </machine>
    </machines>
    
    <wires>
      <wire src="chorus" dst="audio_sink" gain="1.0" pan="0"/>
      <wire src="bd" dst="chorus" gain="1.0" pan="-50"/>
      <wire src="snr" dst="chorus" gain="1.0" pan="-50"/>
      <wire src="hh" dst="chorus" gain="1.0" pan="-50"/>
    </wires>    
  </setup>
  
  <sequence length="64" tracks="2">
    <labels>
      <label time="0" name="start"/>
      <label time="63" name="end"/>
    </labels>
    <tracks>
      <track index="0" machine="bd">
        <position time="0" pattern="bd_beats"/>
        <position time="32" pattern="bd_beats"/>
      </track>
      <track index="1" machine="snr">
        <position time="0" pattern="snr_beats"/>
        <position time="16" pattern="snr_beats"/>
        <position time="32" pattern="snr_beats"/>
        <position time="48" pattern="snr_beats"/>
      </track>
    </tracks>
  </sequence>
  
</buzztard>
