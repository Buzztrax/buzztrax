<?xml version="1.0" encoding="UTF-8"?>

<buzztard
  xmlns="http://www.buzztard.org/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="buzztard.xsd">

  <meta>
    <info>a three voice melody and hihats</info>
    <name>melo 6</name>
    <genre>chip-music</genre>
    <create-dts>2011-10-04T21:44:56Z</create-dts>
    <change-dts>2011-10-04T21:44:56Z</change-dts>
    <bpm>120</bpm>
    <tpb>4</tpb>
    <bars>16</bars>
  </meta>
  
  <setup>
    <machines>
      <machine type="sink" id="audio_sink">
        <properties>
          <property key="xpos" value="0.7"/>
          <property key="ypos" value="0.0"/>
        </properties>
      </machine>
            
      <machine type="source" id="beep1" plugin-name="simsyn" voices="0">
        <globaldata name="volume" value="1.0"/>
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="-0.5"/>
        </properties>
        <patterns>
          <pattern id="beep1::C-4_E-4_G-4" name="C-4_E-4_G-4" length="16">
            <tick time="0">
              <globaldata name="note" value="c-4"/>
              <globaldata name="volume" value="0.85"/>
              <globaldata name="decay" value="0.25"/>
              <globaldata name="wave" value="3"/>
            </tick>
            <tick time="8">
              <globaldata name="note" value="e-4"/>
            </tick>
            <tick time="12">
              <globaldata name="note" value="g-4"/>
            </tick>
          </pattern>
        </patterns>
      </machine>

      <machine type="source" id="beep2" plugin-name="simsyn" voices="0">
        <globaldata name="volume" value="1.0"/>
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="0.0"/>
        </properties>
        <patterns>
          <pattern id="beep2::C-1" name="C-1" length="16">
            <tick time="0">
              <globaldata name="note" value="c-1"/>
              <globaldata name="volume" value="0.4"/>
              <globaldata name="decay" value="1.0"/>
              <globaldata name="wave" value="2"/>
            </tick>
          </pattern>
        </patterns>
      </machine>

      <machine type="source" id="beep3" plugin-name="simsyn" voices="0">
        <globaldata name="volume" value="1.0"/>
        <properties>
          <property key="xpos" value="-0.7"/>
          <property key="ypos" value="0.5"/>
        </properties>
        <patterns>
          <pattern id="beep3::C-2_C-3" name="C-2_C-3" length="16">
            <tick time="0">
              <globaldata name="note" value="c-2"/>
              <globaldata name="volume" value="1.0"/>
              <globaldata name="decay" value="0.15"/>
              <globaldata name="wave" value="2"/>
              <globaldata name="filter" value="1"/>
              <globaldata name="cut-off" value="0.05"/>
              <globaldata name="resonance" value="1.5"/>
            </tick>
            <tick time="2">
              <globaldata name="note" value="c-3"/>
              <globaldata name="cut-off" value="0.1"/>
            </tick>
            <tick time="6">
              <globaldata name="note" value="c-2"/>
              <globaldata name="volume" value="0.7"/>
              <globaldata name="cut-off" value="0.15"/>
              <globaldata name="resonance" value="1.0"/>
            </tick>
            <tick time="6">
              <globaldata name="note" value="c-3"/>
              <globaldata name="cut-off" value="0.2"/>
            </tick>
            <tick time="8">
              <globaldata name="note" value="c-2"/>
              <globaldata name="volume" value="1.0"/>
              <globaldata name="cut-off" value="0.25"/>
              <globaldata name="resonance" value="0.7"/>
            </tick>
            <tick time="10">
              <globaldata name="note" value="c-3"/>
              <globaldata name="cut-off" value="0.3"/>
            </tick>
            <tick time="12">
              <globaldata name="note" value="c-2"/>
              <globaldata name="volume" value="0.7"/>
              <globaldata name="cut-off" value="0.35"/>
              <globaldata name="resonance" value="0.7"/>
            </tick>
            <tick time="14">
              <globaldata name="note" value="c-3"/>
              <globaldata name="cut-off" value="0.4"/>
            </tick>
            <tick time="15">
              <globaldata name="note" value="c-4"/>
              <globaldata name="cut-off" value="0.45"/>
              <globaldata name="resonance" value="0.85"/>
            </tick>
          </pattern>
        </patterns>
      </machine>

      <machine type="processor" id="echo1" plugin-name="audiodelay" voices="0">
        <globaldata name="delaytime" value="25.0"/>
        <globaldata name="feedback" value="75.0"/>
        <globaldata name="drywet" value="50.0"/>
        <properties>
          <property key="xpos" value="0.0"/>
          <property key="ypos" value="-0.5"/>
        </properties>
      </machine>

      <machine type="source" id="hihat" plugin-name="simsyn" voices="0">
        <globaldata name="wave" value="5"/>
        <globaldata name="volume" value="0.6"/>
        <globaldata name="decay" value="0.3"/>
        <globaldata name="filter" value="2"/>
        <globaldata name="cut-off" value="0.8"/>
        <globaldata name="resonance" value="0.7"/>
        <properties>
          <property key="ypos" value="0.5"/>
          <property key="xpos" value="0.0"/>
        </properties>
        <patterns>
          <pattern id="hihat_hats" name="hats" length="16">
            <tick time="2">
              <globaldata name="note" value="c-4"/>
              <globaldata name="decay" value="0.1"/>
            </tick>
            <tick time="6">
              <globaldata name="note" value="c-4"/>
            </tick>
            <tick time="8">
              <globaldata name="note" value="c-4"/>
              <globaldata name="decay" value="0.35"/>
            </tick>
            <tick time="10">
              <globaldata name="note" value="c-4"/>
              <globaldata name="decay" value="0.1"/>
            </tick>
            <tick time="12">
              <globaldata name="note" value="c-3"/>
              <globaldata name="decay" value="0.02"/>
            </tick>
            <tick time="14">
              <globaldata name="note" value="c-4"/>
              <globaldata name="decay" value="0.1"/>
            </tick>
          </pattern>
        </patterns>
      </machine>
    </machines>
    
    <wires>
      <wire src="beep1" dst="echo1" gain="1.0"/>
      <wire src="echo1" dst="audio_sink" gain="1.0"/>
      <wire src="beep2" dst="audio_sink" gain="1.0" panorama="-0.5"/>
      <wire src="beep3" dst="audio_sink" gain="1.0" panorama="+0.5"/>
      <wire src="hihat" dst="audio_sink" gain="1.0"/>
    </wires>
  </setup>
  
  <sequence loop="off" length="100" tracks="6">
    <labels>
      <label time="0" name="start"/>
      <label time="79" name="end"/>
    </labels>
    <tracks>
      <track index="0" machine="beep1">
        <position time="0" pattern="beep1::C-4_E-4_G-4"/>
        <position time="32" pattern="beep1::C-4_E-4_G-4"/>
      </track>
      <track index="1" machine="beep2">
        <position time="0" pattern="beep2::C-1"/>
        <position time="16" pattern="beep2::C-1"/>
        <position time="32" pattern="beep2::C-1"/>
        <position time="48" pattern="beep2::C-1"/>
      </track>
      <track index="2" machine="beep3">
        <position time="0" pattern="beep3::C-2_C-3"/>
        <position time="16" pattern="beep3::C-2_C-3"/>
        <position time="32" pattern="beep3::C-2_C-3"/>
        <position time="48" pattern="beep3::C-2_C-3"/>
        <position time="64" pattern="beep3::C-2_C-3"/>
        <position time="80" pattern="beep3::C-2_C-3"/>
      </track>
      <track index="3" machine="echo1"/>
      <track index="4" machine="hihat">
        <position time="16" pattern="hihat_hats"/>
        <position time="32" pattern="hihat_hats"/>
        <position time="48" pattern="hihat_hats"/>
        <position time="64" pattern="hihat_hats"/>
      </track>
      <track index="5" machine="audio_sink"/>
    </tracks>
  </sequence>

</buzztard>
