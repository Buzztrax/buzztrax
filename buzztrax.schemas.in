<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <!-- ui -->
    <schema>
      <key>/schemas/apps/buzztrax/news-seen</key>
      <applyto>/apps/buzztrax/news-seen</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>0</default>
        <short>Version number for that the user has seen the news</short>
        <long>Tracks for which application version the user has seen the news already.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/missing-machines</key>
      <applyto>/apps/buzztrax/missing-machines</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>List of missing machines to ignore</short>
        <long>
          Comma separated list of machine names, that we will not warn about on startup, if they are missing.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/presented-tips</key>
      <applyto>/apps/buzztrax/presented-tips</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>List of tip-numbers that were shown already</short>
        <long>
          Comma separated list of tip-of-the-day numbers, that the user has seen.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/show-tips</key>
      <applyto>/apps/buzztrax/show-tips</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">
        <default>true</default>
        <short>Show tips on startup</short>
        <long>
          Should the tip-of-the-day dialog be shown everytime the application starts.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/toolbar-hide</key>
      <applyto>/apps/buzztrax/toolbar-hide</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">
        <default>false</default>
        <short>Hide main toolbar</short>
        <long>Should the main application toolbar be hidden.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/statusbar-hide</key>
      <applyto>/apps/buzztrax/statusbar-hide</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">false
        <default>false</default>
        <short>Hide bottom statusbar</short>
        <long>Should the main application statusbar be hidden.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/tabs-hide</key>
      <applyto>/apps/buzztrax/tabs-hide</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">
        <default>false</default>
        <short>Hide main page tabs</short>
        <long>Should the main application notebook pages tabs be hidden.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/grid-density</key>
      <applyto>/apps/buzztrax/grid-density</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default>low</default>
        <short>Machine view grid detail level</short>
        <long>
          How dense the app should draw the grid lines shown in machine view:
          (off,low,medium,high)
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/window/x-pos</key>
      <applyto>/apps/buzztrax/window/x-pos</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>-1</default>
        <short>window-xpos</short>
        <long>Last application window x-position.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/window/y-pos</key>
      <applyto>/apps/buzztrax/window/y-pos</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>-1</default>
        <short>window-ypos</short>
        <long>Last application window y-position.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/window/width</key>
      <applyto>/apps/buzztrax/window/width</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>-1</default>
        <short>window-width</short>
        <long>Last application window width.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/window/height</key>
      <applyto>/apps/buzztrax/window/height</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>-1</default>
        <short>window-height</short>
        <long>Last application window height.</long>
      </locale>
    </schema>
    <!-- audio settings -->
    <schema>
      <key>/schemas/apps/buzztrax/audiosink</key>
      <applyto>/apps/buzztrax/audiosink</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>Which audiosink to use</short>
        <long>Which gstreamer audiosink element should buzztrax applications use.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/audiosink-device</key>
      <applyto>/apps/buzztrax/audiosink-device</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>Which audiosink device to use</short>
        <long>Which audio output device should buzztrax applications use.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/sample-rate</key>
      <applyto>/apps/buzztrax/sample-rate</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>44100</default>
        <short>Which sample-rate to use</short>
        <long>Which sample rate should be used for the generated audio.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/channels</key>
      <applyto>/apps/buzztrax/channels</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>2</default>
        <short>How many audio channels to use</short>
        <long>How many audio channels should be used for the generated audio.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/latency</key>
      <applyto>/apps/buzztrax/latency</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>30</default>
        <short>Target audio latency in ms</short>
        <long>What audio latency should the audio engine be configured for.</long>
      </locale>
    </schema>
    <!-- playback controller -->
    <schema>
      <key>/schemas/apps/buzztrax/playback-controller/coherence-upnp-active</key>
      <applyto>/apps/buzztrax/playback-controller/coherence-upnp-active</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">
        <default>false</default>
        <short>Activate Coherence UPnP based playback controller</short>
        <long>Allows to remote control buzztrax via upnp together by using the coherence backend.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/playback-controller/coherence-upnp-port</key>
      <applyto>/apps/buzztrax/playback-controller/coherence-upnp-port</applyto>
      <owner>buzztrax</owner>
      <type>int</type>
      <locale name="C">
        <default>7654</default>
        <short>Socket port for the Coherence UPnP based playback controller</short>
        <long>The port number for the communication with the coherence backend.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/playback-controller/jack-transport-master</key>
      <applyto>/apps/buzztrax/playback-controller/jack-transport-master</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">
        <default>false</default>
        <short>Sync other jack clients to buzztrax playback state</short>
        <long>Allows to remote control jack applications from buzztrax.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/playback-controller/jack-transport-slave</key>
      <applyto>/apps/buzztrax/playback-controller/jack-transport-slave</applyto>
      <owner>buzztrax</owner>
      <type>bool</type>
      <locale name="C">
        <default>false</default>
        <short>Sync buzztrax to the playback state other jack clients</short>
        <long>Allows to remote control buzztrax via jack transport.</long>
      </locale>
    </schema>
    <!-- directory settings -->
    <schema>
      <key>/schemas/apps/buzztrax/song-folder</key>
      <applyto>/apps/buzztrax/song-folder</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>Default directory for songs</short>
        <long>
          Default directory for filechoosers, when loading existing or saving new songs.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/record-folder</key>
      <applyto>/apps/buzztrax/record-folder</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>Default directory for recordings</short>
        <long>
          Default directory song recordings.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/buzztrax/sample-folder</key>
      <applyto>/apps/buzztrax/sample-folder</applyto>
      <owner>buzztrax</owner>
      <type>string</type>
      <locale name="C">
        <default></default>
        <short>Default directory for digital waveforms</short>
        <long>
          Default directory for the sample browser in the wavetable view.
        </long>
      </locale>
    </schema>
    
  </schemalist>
</gconfschemafile>
