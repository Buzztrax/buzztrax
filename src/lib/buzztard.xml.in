<?xml version="1.0" encoding="UTF-8"?>

<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="audio/x-bzt">
    <glob pattern="*.bzt"/>
    <magic>
      <match type="string" value="PK" offset="0:2"/>
    </magic>
    <magic priority="100">
      <match type="string" value="song.xml" offset="0:40"/>
    </magic>
    <comment>buzztard song</comment>
    <!--desktop:can-edit-with>bt-edit.desktop</desktop:can-edit-with-->
  </mime-type>
  <mime-type type="audio/x-bzt-xml">
    <glob pattern="*.xml"/>
    <magic priority="100">
      <match type="string" value="buzztard" offset="0:100"/>
      <match type="string" value="http://www.buzztard.org/" offset="0:100"/>
    </magic>
    <comment>buzztard song (xml)</comment>
    <!--desktop:can-edit-with>bt-edit.desktop</desktop:can-edit-with-->
  </mime-type>
</mime-info>
