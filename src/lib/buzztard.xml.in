<?xml version="1.0" encoding="UTF-8"?>

<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="audio/x-bzt">
    <glob pattern="*.bzt"/>
    <magic priority="80">
      <match type="string" value="; BztProject" offset="0:64"/>
      <match type="string" value="(bzt-version)" offset="0:64"/>
    </magic>
    <comment>BZT song</comment>
    <!--desktop:can-edit-with>bt-edit.desktop</desktop:can-edit-with-->
  </mime-type>
</mime-info>
