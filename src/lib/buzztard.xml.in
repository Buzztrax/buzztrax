<?xml version="1.0" encoding="UTF-8"?>

<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="audio/x-bzt">
    <sub-class-of type="application/zip"/>
    <glob pattern="*.bzt"/>
    <magic priority="100">
      <match type="string" value="song.xml" offset="0:40"/>
    </magic>
    <comment>buzztard song</comment>
  </mime-type>
  <mime-type type="audio/x-bzt-xml">
    <sub-class-of type="application/xml"/>
    <glob pattern="*.xml"/>
    <magic priority="100">
      <match type="string" value="buzztard" offset="0:100"/>
      <match type="string" value="http://www.buzztard.org/" offset="0:100"/>
    </magic>
    <comment>buzztard song (xml)</comment>
  </mime-type>
</mime-info>
